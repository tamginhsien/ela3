                android:layout_height="wrap_content"
                android:text="@string/textviewSubmenu3_2_7"
                android:textColor="@color/black"
                android:justificationMode="inter_word"
                />

            <com.ortiz.touchview.TouchImageView
                android:id="@+id/connection_examples_3.2.2"
                android:layout_width="match_parent"
                android:layout_height="180dp"
                android:layout_gravity="center"
                android:contentDescription="@string/connection_examples_3_2_2"
                android:src="@drawable/connection_examples3_2_2" />

            <TextView
                android:id="@+id/textView_submenu3.2.8"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="@string/textviewSubmenu3_2_8"
                android:textColor="@color/black"
                android:justificationMode="inter_word"
                android:autoLink="web"
                />

            <Button
                android:id="@+id/button_comment_5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/comment_example_5"
                android:textSize="16sp" />


            <TextView
                android:id="@+id/textView_submenu3.2.9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/textviewSubmenu3_2_9"
                android:textColor="@color/black"
                android:justificationMode="inter_word"
                />

            <TextView
                android:id="@+id/textView_coding3.2.3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:text="@string/textviewCoding3_2_3"
                android:textColor="@color/black"
                android:justificationMode="inter_word"
                android:textIsSelectable="true"
                />

            <TextView
                android:id="@+id/textView_submenu3.2.10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/textviewSubmenu3_2_10"
                android:textColor="@color/black"
                android:justificationMode="inter_word"
                />

            <com.ortiz.touchview.TouchImageView
                android:id="@+id/schematic_examples3.2.3"
                android:layout_width="match_parent"
                android:layout_height="220dp"
                android:contentDescription="@string/schematic_examples3_2_3"
                android:src="@drawable/schematic_examples3_2_3" />

            <TextView
                android:id="@+id/textView_submenu3.2.11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/textviewSubmenu3_2_11"
                android:textColor="@color/black"
                android:justificationMode="inter_word"
                />

            <com.ortiz.touchview.TouchImageView
                android:id="@+id/connection_examples_3.2.3"
                android:layout_width="match_parent"
                android:layout_height="180dp"
                android:layout_gravity="center"
                android:contentDescription="@string/connection_examples_3_2_3"
                android:src="@drawable/connection_examples3_2_3" />


            <TextView
                android:id="@+id/textView_submenu3.2.12"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="@string/textviewSubmenu3_2_12"
